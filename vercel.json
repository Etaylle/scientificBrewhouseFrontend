{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://959d3bb0f1a5.ngrok-free.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "ngrok-skip-browser-warning",
          "value": "true"
        }
      ]
    }
  ]
}
